<%- include('../header') %>
<div class="container my-3">
  <h3>Detail edit page</h3>
  <div class="row">
    <div class="col-8 mx-auto">
      <div class="card" style="width: 18rem">
        <ul class="list-group list-group-flush">
          <li class="list-group-item">
            <strong>NIM:</strong> <%= praktikans.nim || null%>
          </li>
          <li class="list-group-item">
            <strong>Nama:</strong> <%= praktikans.nama %>
          </li>
          <li class="list-group-item">
            <strong>Tahun Masuk:</strong> <%= praktikans.tahun_masuk %>
          </li>
          <li class="list-group-item">
            <strong>Jurusan:</strong> <%= praktikans.jurusan %>
          </li>
          <li class="list-group-item">
            <strong>kelas :</strong><br /><% result.matkuls.forEach((el,i) => {
            %>
            <small class="btn btn-warning btn-sm"
              ><%= el.nama%>: <%= result.kelas[i] %></small
            >
            <%})%>
          </li>
        </ul>
        <div class="card-footer">
          <a
            href="/mataPraktikum/add/<%= praktikans.id %>"
            class="btn btn-primary"
            >tambah kelas praktikum</a
          >
        </div>
      </div>
    </div>
  </div>
</div>

<%- include('../footer') %>
